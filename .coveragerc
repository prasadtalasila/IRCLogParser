[run]
omit =
    # omit anything in a .local directory anywhere
    */.local/*
    # omit everything in /usr
    /usr/*
    # omit testfile
    ./test/*
    */__init__.py
    */ext/*
    */config.py
    # remove drivers from coverage
    ./slack.py
    ./ubuntu.py
    ./scummvm.py
    ./sample.py
    ./setup.py
